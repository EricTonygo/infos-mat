        <!-- Block contenu  Start-->


<div class="box box-success">
    <div class="box-header">
        <h3>Créer un nouvel utilisateur</h3>
    </div>

<form role="form" action="{{ path('fos_user_registration_register') }}" {{ form_enctype(form) }} method="POST" class="fos_user_registration_register">
        
   <div class="box-body">
        <div class="row">
            <div class="col-md-8 col-md-offset-2">
                <div class="form-group">
                    <label for="">Privilège</label>
                    {{ form_widget(form.roles)}}
                </div>
                <div class="form-group">
                  {{ form_label(form.nom,"Nom") }}
                  {{ form_errors(form.nom)}}
                  {{ form_widget(form.nom,{'attr':{'class':'form-control'}})}}
                </div>
                <div class="form-group">
                  {{ form_label(form.email,"Email") }}
                  {{ form_errors(form.email)}}
                  {{ form_widget(form.email,{'attr':{'class':'form-control'}})}}
                </div>
                <div class="form-group">
                   
                   {{ form_label(form.username,"Nom d'utilisateur") }}
                  {{ form_errors(form.username)}}
                  {{ form_widget(form.username,{'attr':{'class':'form-control'}})}}
                </div>
                <div class="form-group">
                    
                  {{ form_label(form.plainPassword.first,"Mot de passe") }}
                 {{ form_errors(form.plainPassword.first)}}
                  {{ form_widget(form.plainPassword.first,{'attr':{'class':'form-control'}})}}
                </div>
                <div class="form-group">
                   
                  {{ form_label(form.plainPassword.second,"Confirmation mot de passe") }}
                 {{ form_errors(form.plainPassword.second)}}
                  {{ form_widget(form.plainPassword.second,{'attr':{'class':'form-control'}})}}
                </div>
            </div>

            <div class="col-md-8 col-md-offset-2">
                <div class="img-thumbnail img-responsive img-preview">
                    <img alt="" src="" id="preview-image" width="100%"/>
                </div>
                <div class="form-group">
                    {{ form_label(form.file, "Photo de profil")}}
                    {{form_errors(form.file)}}
                    {{ form_widget(form.file, { 'attr' : {'class' : 'inputfile' }}) }}
                    <p class="help-block">Sélectionner une photo de profil.</p>
                </div>
            </div>
           <div class="col-md-8 col-md-offset-2 form-group">
                    {{form_rest(form)}}
            </div>
        </div>
    </div>
    <div class="box-footer col-md-8 col-md-offset-2">
      <button type="reset" class="btn btn-danger ">Reinitialiser</button>
      <button type="submit" class="btn btn-success">Enregistrer</button>
    </div>
</form>
</div>
