{% extends "::base.html.twig" %}

{% block title %}GMTRAV{% endblock %}
{# core CSS #}
{% block stylesheets %}
    {# Bootstrap #}
    <link href="{{ asset('bundles/nngenieinfosmat/vendors/bootstrap/dist/css/bootstrap.min.css') }}" rel="stylesheet">
     {# Font Awesome #}
    <link href="{{ asset('bundles/nngenieinfosmat/vendors/font-awesome/css/font-awesome.min.css') }}" rel="stylesheet">

    {# Custom Theme Style #}
    <link href="{{ asset('bundles/nngenieinfosmat/css/custom.css') }}" rel="stylesheet">
    <link href="{{ asset('bundles/nngenieinfosmat/css/personal.css') }}" rel="stylesheet">
{% endblock %}


{% block content_body %}
