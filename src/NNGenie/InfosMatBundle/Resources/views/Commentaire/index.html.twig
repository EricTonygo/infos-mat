{% extends "::base.html.twig" %}

{% block title %}NNGenieInfosMatBundle:CommentaireController:index{% endblock %}

{% block body %}
<h1>Welcome to the CommentaireController:index page</h1>
<ul>
  {% for commentaire in commentaires %}
    <li>{{ commentaire.description }}</li>
    <li>{{ commentaire.materiel.nom }}</li>
  {% else %}
    <li>Pas de commentaires sur ce materiel.</li>
  {% endfor %}
</ul> 
{% endblock %}
